<?xml version='1.0' encoding='utf-8'?>
<widget id="com.webcinizim.tatipet" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TatiPet</name>
    <description>
    Tati Pet Kuaför video yükleme uygulaması
  </description>
    <author email="bilgi@webcinizim.com" href="http://webcinizim.com">
    webcinizim.com web çözümleri
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon/android/icon.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="180" src="res/icon/ios/Icon-60@3x.png" width="180" />
        <icon height="60" src="res/icon/ios/Icon-60.png" width="60" />
        <icon height="120" src="res/icon/ios/Icon-60@2x.png" width="120" />
        <icon height="76" src="res/icon/ios/Icon-76.png" width="76" />
        <icon height="152" src="res/icon/ios/Icon-76@2x.png" width="152" />
        <icon height="167" src="res/icon/ios/Icon-83.5@2x.png" width="167" />
        <icon height="29" src="res/icon/ios/Icon-small.png" width="29" />
        <icon height="58" src="res/icon/ios/Icon-small@2x.png" width="58" />
        <icon height="87" src="res/icon/ios/Icon-small@3x.png" width="87" />
        <icon height="40" src="res/icon/ios/Icon-40.png" width="40" />
        <icon height="80" src="res/icon/ios/Icon-40@2x.png" width="80" />
        <icon height="120" src="res/icon/ios/Icon-40@3x.png" width="120" />
        <splash height="480" src="res/splash/ios/Default~iphone.png" width="320" />
        <splash height="960" src="res/splash/ios/Default@2x~iphone.png" width="640" />
        <splash height="1024" src="res/splash/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="2048" src="res/splash/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="768" src="res/splash/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="res/splash/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1136" src="res/splash/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/splash/ios/Default-667h.png" width="750" />
        <splash height="2208" src="res/splash/ios/Default-736h.png" width="1242" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="" />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="" />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.3" />
    <plugin name="cordova-plugin-activityindicator" spec="https://github.com/kartagis/cordova-plugin-activityindicator.git" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^6.3.0" />
</widget>
